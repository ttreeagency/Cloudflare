prototype(Ttree.Cloudflare:Cache) {
	@class = '\\Ttree\\Cloudflare\\TypoScript\\CacheImplementation'

	documentNode = ${documentNode}

	# this can be use to disable the caching features
	# Warning: currently CloudFlare will continue to cache your page
	enable = TRUE

	# apiKey and email are configured in your Settings.yaml
	zone = 'need-to-be-configured'
}